;### DATABASE CONNECTION ###;
dbName='<%= @dbName %>'
dbUser='<%= @dbUser %>'
dbPassword='<%= @dbPassword %>'
dbHost='<%= @dbHost %>'
dbDriver='<%= @dbDriver %>'

adminAuthenticationClass='\Jazzee\AdminAuthentication\NoAuthentication'
noAuthIpAddresses='<%= @safeIps %>'

adminDirectoryClass='\Jazzee\AdminDirectory\Local'

maximumApplicantFileUploadSize=4M
defaultApplicantFileUploadSize=2M
maximumAdminFileUploadSize=5M

allowApplicantNameChange=true
allowApplicantEmailChange=true
allowApplicantPasswordChange=true
allowApplicantPrintApplication=true

;#Public OPENSSL PKI certificate is used to encrypt applicant input for especially sensitive data like SSNs
;#For security only the public key should be stored on the Jazzee System and decryption should happen offline
publicKeyCertificatePath='<%= @sslCert %>'

;### DEVELOPMENT OPTIONS ###;
status=DEVELOPMENT

adminCronAllowed='<%= @safeIps %>'

varPath=<%= @varPath %>

mailOverrideToAddress='vagrant@localhost.localdomain'

virusScanUploads=false

allowScramble=true